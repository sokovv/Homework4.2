{
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "browser": true,
        "jest": true
    },
    "rules": {
        "import/extensions": [
            "error",
            "ignorePackages"
        ],
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ]
    }
}